<h1>Listing userprofiles</h1>

<table>
  <tr>
    <th>First name</th>
    <th>Last name</th>
    <th>Physical address</th>
    <th>Unit no</th>
    <th>Postal zip code</th>
    <th>City</th>
    <th>Country</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @userprofiles.each do |userprofile| %>
  <tr>
    <td><%= userprofile.first_name %></td>
    <td><%= userprofile.last_name %></td>
    <td><%= userprofile.physical_address %></td>
    <td><%= userprofile.unit_no %></td>
    <td><%= userprofile.postal_zip_code %></td>
    <td><%= userprofile.city %></td>
    <td><%= userprofile.country %></td>
    <td><%= link_to 'Show', userprofile %></td>
    <td><%= link_to 'Edit', edit_userprofile_path(userprofile) %></td>
    <td><%= link_to 'Destroy', userprofile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Userprofile', new_userprofile_path %>
