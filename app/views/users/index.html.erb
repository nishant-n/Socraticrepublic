<table border="1">
	<tr>
		 <th>Email</th>
		 <th>User Id</th>
		 <th>Action</th>
	</tr>
<% @users.each do |user|%>
 <tr>
 	<td>
  <%=user.email%></td>
  <td><%=user.id%></td>
  <td><%= link_to 'Delete',user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
<%end%>
</table>

<p> Add Email </p>
<%= form_for @user, :url => create_user_users_path do |f| %>
 	
 	  <%= f.email_field :email, :autofocus => true %></div><br/>
   <%= f.submit "Add Email" %>
<%end%>

<%= link_to "Program functionality" ,user_profile_path(current_user)%><br/>

<%= link_to "Program Display"  %><br/>

<%= link_to "logout" , destroy_user_session_path ,:method => :delete %>